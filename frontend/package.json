{
  "author": "Raul Farias S",
  "name": "fontend-base",
  "description": "Proyescto Base Front",
  "keywords": [],
  "private": true,
  "version": "0.0.1",
  "license": "MIT",
  "type": "module",
  "packageManager": "pnpm@10.13.1",
  "engines": {
    "node": "22.17.1"
  },
  "eslintConfig": {
    "extends": [
      "./node_modules/standard/eslintrc.json"
    ],
    "env": {
      "node": true,
      "browser": true,
      "jest": true
    }
  },
  "lint-staged": {
    "*.css": "stylelint",
    "*.{js,jsx}": "standard"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint:js": "standard",
    "lint:js:fix": "standard --fix",
    "lint:css": "stylelint '**/*.css'",
    "lint:css:fix": "stylelint '**/*.css' --fix",
    "lint": "npm run lint:css && npm run lint:js",
    "lint:fix": "npm run lint:css:fix && npm run lint:js:fix",
    "lint:check": "node lint-check.js",
    "lint:auto": "node lint-check.js --fix",
    "test": "vitest",
    "preview": "vite preview",
    "prepare": "husky"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "bootstrap": "^5.3.7",
    "react": "19.1.0",
    "react-bootstrap-icons": "^1.11.6",
    "react-dom": "19.1.0",
    "react-router-dom": "^7.8.2",
    "react-toastify": "^11.0.5",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@testing-library/jest-dom": "6.6.4",
    "@testing-library/react": "16.3.0",
    "@testing-library/user-event": "14.6.1",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "@vitejs/plugin-react": "4.6.0",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "3.2.4",
    "husky": "9.1.7",
    "jsdom": "26.1.0",
    "lint-staged": "16.1.2",
    "msw": "2.10.4",
    "standard": "17.1.2",
    "stylelint": "16.23.0",
    "stylelint-config-standard": "39.0.0",
    "vite": "7.0.4",
    "vitest": "3.2.4"
  }
}
